{
  "version": 2,
  "name": "izerwaren-marine-shop",
  "framework": "nextjs",
  "rootDirectory": "apps/frontend",
  "regions": ["iad1"],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
}